Comment'\n\nconst Comments = (props) => {\n\n    const [comments, setComments] = useState([])\n    const [newComment, setNewComment] = useState('')\n    \n    useEffect(()=> {\n        getComments()\n    }, [])\n\n    const getComments= ()=> {\n        axios\n        .get(`/api/comments/${props.articleId}`)\n        .then((res) => {\n            setComments(res.data)\n        })\n        .catch(err => console.log(err))\n    }\n\n    const addComment = () => {\n        axios\n        .post(`/api/comments/${props.articleId}`, {newComment: newComment})\n        .then (res => {\n            getComments()\n            setNewComment('')\n        })\n        .catch(err => alert('Please login to add comments. Thank you.'))\n    }\n    \n    const updateComment = (commentId, updateCommentStr) => {\n        axios\n        .put(`/api/comments/${commentId}`, {updatedComment: updateCommentStr})\n        .then (res => {\n            getComments()\n        })\n    }\n\n    const deleteComment = (commentId) => {\n        axios\n        .delete(`/api/comments/${commentId}`)\n        .then (res => {\n            getComments()\n        })\n    }\n\n\n    const mappedComments = comments.map((comment) => {\n        console.log(comment)\n        return (\n        <div>\n            <Comment \n            comment = {comment} \n            updateComment = {updateComment}\n            deleteComment = {deleteComment}\n            key= {comment.comments_id}\n            />\n            \n        </div>\n        )\n    })\n\n    return (\n        <div className='comments'>\n            <div className='add-comment'>\n                <h5>Please leave your comments here. Remember to be respectful to other subscribers.</h5>\n            </div>\n\n            <input\n            className='input-comment'\n            value={newComment}\n            onChange={e => setNewComment(e.target.value)}/>\n            <button\n            type=\"reset\" value=\"Reset\"\n            className='btn-create-account'\n            onClick = {addComment}>Add</button>\n          {mappedComments}\n        </div>\n    )\n}\n\nexport default Comments","/Users/nicholasloverde/web-projects/devmountain/unit-7/economist-clone/src/components/Comments/Comment.js",["87"],"import React, {useState} from 'react'\nimport {withRouter} from 'react-router-dom'\n\nconst Comment = (props) => {\n    const {comment} = props\n    const [body, setBody] = useState('')\n    // const [editToggle, setEditToggle] = useState(false)\n\n    //create ternary if id's match show buttons if now don't show it\n\n    return (\n        <div className='comment'>\n            <div className='comment-author'>\n                <h5>{comment.first_name, comment.last_name}</h5>\n            </div>\n\n            <div className='comment-update'>\n                <div className='comment-author'>\n                    <p>{comment.body}</p>\n                </div>\n\n\n                <div>\n                <input\n                value={body}\n                onChange={e => setBody(e.target.value)}/>\n                <button\n                className=\"btn-create-account\"\n                onClick = {() => props.updateComment(comment.comments_id, body)}>Update\n                </button>\n\n\n                <button\n                className=\"btn-create-account\"\n                onClick = {() => props.deleteComment(comment.comments_id)}>Delete</button>\n                </div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default withRouter(Comment)\n\n","/Users/nicholasloverde/web-projects/devmountain/unit-7/economist-clone/src/components/Article/ArticleComments.js",["88"],"/Users/nicholasloverde/web-projects/devmountain/unit-7/economist-clone/src/redux/store.js",[],"/Users/nicholasloverde/web-projects/devmountain/unit-7/economist-clone/src/redux/subscriberReducer.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":4,"column":8,"nodeType"